{
  "name": "apiconnect-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "node --max-old-space-size=16384 ./node_modules/vitest/vitest.mjs run --pool=forks --poolOptions.forks.singleFork=true",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=json --outputFile=coverage/test-results.json",
    "audit:dead-code": "node scripts/audit-dead-code.cjs",
    "audit:dependencies": "node scripts/analyze-dependencies.cjs",
    "audit:duplicates": "jscpd --config .jscpd.json",
    "audit:duplicates:html": "jscpd --config .jscpd.json --reporters html",
    "clean:unused": "npm run audit:dead-code && npm run lint:fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.6.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/html2canvas": "^0.5.35",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/node": "^24.2.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.2.15",
    "@types/react-query": "^1.2.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "html2canvas": "^1.4.1",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jest-image-snapshot": "^6.5.1",
    "jscpd": "^4.0.5",
    "jsdom": "^23.0.1",
    "lint-staged": "^16.1.6",
    "msw": "^2.10.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
